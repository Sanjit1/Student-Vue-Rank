{
	"manifest_version": 2,
	"name": "Student VUE Show Rank",
	"version": "1.0",
	"description": "Shows your School Rank if Student Vue hides it.",
	"page_action": {
		"default_icon": {
			"16": "images/icon-16.png",
			"32": "images/icon-32.png",
			"48": "images/icon-48.png",
			"128": "images/icon-128.png"
		}
	},
	"content_scripts": [
		{
			"css": [
				"styles.css"
			],
			"js": [
				"content.js"
			],
			"matches": [
				"https://sis.powayusd.com/*"
			]
		}
	]
}